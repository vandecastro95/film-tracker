{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.0.0"
      }
    ],
    "@babel/typescript",
    "@babel/react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@loadable/babel-plugin",
    "@babel/proposal-class-properties",
    "@babel/syntax-dynamic-import"
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}
